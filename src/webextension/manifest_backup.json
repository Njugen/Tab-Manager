{
    "manifest_version": 3,
    "name": "TabSera",
    "version": "0.0.1",
    "description": "Providing great overview and simple tools for efficient tab management.",
    "permissions": [
        "history",
        "tabs",
        "storage",
        "sidePanel"
    ],
    "action": {
        "default_title": "TabSera",
        "default_popup": "popup.html"
    },
    "icons": {
        "128": "brand/icon-128.png",
        "48": "brand/icon-48.png"
    },
    "side_panel": {
        "default_path": "sidepanel.html"
    },
    "options_ui": {
        "page": "options.html",
        "open_in_tab": true
    },
    "background": {
        "service_worker": "background.js"
    },
    "author": "Thai Nguyen"
}