{
    "manifest_version": 3,
    "name": "TabSera",
    "version": "0.0.1",
    "description": "Providing great overview and simple tools for efficient tab management.",
    "permissions": [
        "bookmarks",
        "tabs",
        "storage"
    ],
    "action": {
        "default_title": "TabSera",
        "default_popup": "popup.html"
    },
    "options_ui": {
        "page": "options.html",
        "open_in_tab": true
    },
    "background": {
        "service_worker": "background.js"
    },
    "content_scripts": [{
        "matches": ["https://*.google.com/*"],
        "js": ["contentScript.js"]
    }],
    "author": "Thai Nguyen"
}