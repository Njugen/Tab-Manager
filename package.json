    "autoprefixer": "^10.4.15",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.8.1",
    "css-modules-typescript-loader": "^4.0.1",
    "jest-chrome": "^0.8.0",
    "postcss": "^8.4.29",
    "postcss-loader": "^7.3.3",
    "postcss-nesting": "^12.0.1",
    "react": "^18.2.0",
    "react-css-modules": "^4.7.11",
    "react-dom": "^18.2.0",
    "react-redux": "^8.1.2",
    "react-router-dom": "^6.15.0",
    "react-scripts": "5.0.1",
    "redux-mock-store": "^1.5.4",
    "redux-thunk": "^2.4.2",
    "sass": "^1.66.1",
