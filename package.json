    "style-loader": "^3.3.3",
    "tailwindcss": "^3.3.3",
    "ts-loader": "^9.4.4",
    "typescript": "^4.9.5",
    "web-vitals": "^2.1.4",
    "webextension-polyfill": "^0.10.0",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4",
    "webpack-merge": "^5.9.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build-prod": "webpack --progress --config webpack.prod.js",
    "build-dev": "webpack --progress --config webpack.dev.js",
    "eject": "react-scripts eject",
    "dev": "webpack",
    "watch": "webpack --watch --progress --config webpack.dev.js",
    "test": "react-scripts test --coverage --watchAll --collectCoverageFrom='src/components/**/*.{ts,tsx}' --collectCoverageFrom='src/tools/**/*.{ts,tsx}' --collectCoverageFrom='src/redux/**/*.{ts,tsx}' --collectCoverageFrom='src/views/**/*.{ts,tsx}' --collectCoverageFrom='src/images/**/*.{ts,tsx}'"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@babel/plugin-transform-private-property-in-object": "^7.22.11",
    "@testing-library/dom": "^9.3.4",
    "@testing-library/user-event": "^14.5.2",
    "@types/redux-mock-store": "^1.0.6"
  },
  "jest": {
    "coveragePathIgnorePatterns": [
      "./redux/",
      "./tools/testing/"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 80,
        "lines": 80,
        "statements": -10
      }
    }
  }
}
